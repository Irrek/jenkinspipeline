pipeline {
  agent any
  tools{
        maven 'localMaven'
        jdk 'JDK 8'
  }
  stages{
    stage('Build'){
      steps {
        sh 'mvn clean package'
      }
      post {
        success {
          echo 'Now Archiving...'
          archiveArtifacts artifacts: '**/*.war'
        }
      }
    }
  }
}
